{
  "manifest_version": 3,
  "name": "ARDY",
  "version": "1.0",
  "description": "Display adjusted video length based on playback rate multiplier",
  "permissions": ["scripting"],
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
