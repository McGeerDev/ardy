{
  "manifest_version": 3,
  "name": "ARDY",
  "version": "1.0",
  "description": "Display adjusted video length based on playback rate multiplier",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
